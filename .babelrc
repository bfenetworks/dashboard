{
    "presets": ["env", "stage-2", ["es2015", { "modules": false }]],
    "plugins": [
        "transform-runtime",
        "transform-vue-jsx",
        [
            "component",
            {
                "libraryName": "element-ui",
                "styleLibraryName": "theme-chalk"
            }
        ]
    ],
    "comments": false,
    "env": {
        "test": {
            "plugins": ["istanbul"]
        }
    }
}
